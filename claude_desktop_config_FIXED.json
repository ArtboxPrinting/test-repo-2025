{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "C:\\Users\\alan\\Documents",
        "C:\\Users\\alan\\Downloads", 
        "C:\\Users\\alan\\Desktop",
        "C:\\Users\\alan\\Documents\\alan-projects"
      ]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "YOUR_GITHUB_TOKEN_HERE"
      }
    },
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--access-token",
        "YOUR_SUPABASE_TOKEN_HERE"
      ]
    },
    "brave-search": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": "YOUR_BRAVE_API_KEY_HERE"
      }
    },
    "web-fetch": {
      "command": "npx",
      "args": [
        "-y",
        "fetch-mcp"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "shell": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-shell"
      ]
    },
    "vercel": {
      "command": "npx",
      "args": [
        "-y",
        "github:Quegenx/vercel-mcp-server"
      ],
      "env": {
        "VERCEL_ACCESS_TOKEN": "YOUR_VERCEL_TOKEN_HERE"
      }
    },
    "redis": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-redis",
        "redis://localhost:6379"
      ]
    },
    "deep-research": {
      "command": "npx",
      "args": [
        "-y",
        "github:JoshuaLelon/deep-research-mcp"
      ],
      "env": {
        "OPENAI_API_KEY": "YOUR_OPENAI_API_KEY_HERE"
      }
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@context7/mcp-server"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-playwright"
      ]
    },
    "desktop-commander": {
      "command": "npx",
      "args": [
        "-y",
        "desktop-commander"
      ]
    },
    "mcp-installer": {
      "command": "npx",
      "args": [
        "-y",
        "@anaisretter/mcp-installer"
      ]
    }
  }
}